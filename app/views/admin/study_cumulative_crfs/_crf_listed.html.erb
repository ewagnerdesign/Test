<li id="<%= dom_id_nested(:item, @study, form) %>"
  <%= highlightable(dom_id_nested(:tools, @study, form), true) %>
  <%= clickable admin_study_crf_path(@study, form.id) %>   >
  <div class="tools" id="<%= dom_id_nested(:tools, @study, form) %>"<%= form.id %> style="display:none;">
    <%= link_to_remote 'Show', {:method => :get, :url => admin_study_crf_path(@study, form)}, {:onclick => "event.cancelBubble=true;"} %> |
    <%= link_to_remote 'Remove from Visit',
       {:method=> :delete, :url => admin_study_crf_path(@study, form)},
       {:onclick => "event.cancelBubble=true;"} %>
  </div>
  <div class="strip">
    <%= form.cumulative? ? "Cumulative" : "eFORM" %>
  </div>
  <div class="indent">
    <strong><%= highlight_search_result(form.name, @current_query) %></strong>
    <%= '(' + form.domain.try(:name) + ')' unless form.domain.nil? %>
    <br/>
  </div>
</li>
