<div class="multiField">
  <% control[:options].each do |option| %>
  <label class="inlineLabel">
    <%= check_box_tag "", option[0],
                      former_control_selected?(control, option[0]),
                      former_control_identifiers(control, option[0].to_i)  %>
    <span><%= option[1] %></span>
  </label>
  <% end %>
</div>
