<div class="block-container" style="border-color:LimeGreen;">
  <div class="title">Edit contact - <%= @user.full_name %> </div>
  <%= link_to_close(edit_admin_site_contact_path(@site, @user)) %>
  <div id="<%= dom_id(@user) %>" class="input-form crud-form">
  <% remote_form_for [:admin, @site, @user], :url => admin_site_contact_path(@site, @user) do |f| %>
    <%= render :partial => "admin/users/form", :locals => {:f => f} %>
  <% end %>
  </div>
</div>
