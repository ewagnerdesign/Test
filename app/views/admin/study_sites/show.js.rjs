if params[:cancel] == "true"
  page[dom_id(@study, dom_id(@site))].replace :partial => "site"
else
  page[dom_id(@study, dom_id(@site))].replace_html :partial => "show"
  page << "$('##{dom_id(@study, dom_id(@site))}').get(0).onmouseover = null"
  page << "$('##{dom_id(@study, dom_id(@site))}').get(0).onclick = null"
  #TODO: set white color
  page.call "$('##{dom_id(@study, dom_id(@site, :show))}').tabify"
  #page[dom_id(dom_id(@site)].onclick = ""
end
